<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hongxing.mapper.OrganMapper">

    <select id="getOrganByOgIndex" resultType="com.example.hongxing.entity.OrganEntity">
        select
            *
        from organ
        WHERE OGINDEX = #{ogIndex}
    </select>

    <select id="getOrganByOgId" resultType="com.example.hongxing.entity.OrganEntity">
        select
            *
        from organ
        WHERE  OGID = #{ogId}
    </select>

</mapper>